{
    "name": "dennisvanbeersel/symfony-logger-client",
    "description": "Symfony bundle for Application Logger error tracking platform",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "error-tracking", "monitoring", "logging", "sentry"],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/uid": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "symfony/security-bundle": "^6.4|^7.0",
        "symfony/twig-bundle": "^6.4|^7.0",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ApplicationLogger\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationLogger\\Bundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix",
        "phpstan": "phpstan analyse",
        "lint": [
            "@cs-check",
            "@phpstan"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
